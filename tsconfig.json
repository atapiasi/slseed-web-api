{
  "compilerOptions": {
    "outDir": ".build",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "es2020"
    ],
    "typeRoots": [
      "types",
      "node_modules/@types"
    ]
  },
  "include": [
    "service/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
