{
  "name": "@fiquu/slseed-web-api",
  "title": "Slseed Web API",
  "description": "Serveless Seed Web API project",
  "author": "fiquu <hi@fiquu.dev> (https://github.com/fiquu)",
  "homepage": "https://github.com/fiquu/slseed-web-api",
  "main": "npm start",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/fiquu/slseed-web-api/issues"
  },
  "directories": {
    "test": "test"
  },
  "contributors": [
    "Santiago G. Mar√≠n <santiago.marin@final.cl> (https://github.com/stgogm)"
  ],
  "keywords": [
    "slseed",
    "serverless",
    "sls",
    "api",
    "seed",
    "project"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:fiquu/slseed-web-api.git"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "nyc": {
    "reporter": [
      "html"
    ],
    "include": [
      "service"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "test:unit": "npm run test:components && npm run test:schemas && npm run test:functions",
    "test:functions": "sls invoke test --stage local --path test/functions",
    "test:components": "cross-env NODE_ENV=local mocha test/components",
    "test:schemas": "cross-env NODE_ENV=local mocha test/schemas",
    "postversion": "git push --tags",
    "pretest": "npm run clean",
    "test": "nyc npm run test:unit",
    "db-sync": "ts-node scripts/sync-db-indexes.ts",
    "domain": "sls create_domain --stage production",
    "clean": "rimraf .build/",
    "prestart": "npm run clean",
    "start": "sls offline start --stage local",
    "setup:stack": "slseed-utils --do setup-stack",
    "setup:env": "slseed-utils --do setup-env",
    "setup": "slseed-utils",
    "predeploy": "npm run clean",
    "deploy": "slseed-utils --do deploy",
    "check": "npm-check --update"
  },
  "dependencies": {
    "@fiquu/database-manager-mongoose": "^1.0.1",
    "@fiquu/lambda-http-event-handler": "^1.0.0",
    "@fiquu/logger": "^1.0.1",
    "@fiquu/schema-loader-mongoose": "^1.0.0",
    "aws-sdk": "^2.633.0",
    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "merge-graphql-schemas": "^1.7.6",
    "mongoose": "^5.9.3",
    "object-path": "^0.11.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@fiquu/eslint-config": "^2.0.0",
    "@types/aws-lambda": "^8.10.46",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^7.0.2",
    "@types/ws": "^7.2.2",
    "amazon-cognito-identity-js-node": "0.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "fi-is": "^4.0.0",
    "inquirer": "^7.0.6",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "ora": "^4.0.3",
    "rimraf": "^3.0.2",
    "serverless": "^1.65.0",
    "serverless-domain-manager": "^3.3.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-mocha-plugin": "git+https://github.com/nordcloud/serverless-mocha-plugin.git",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-prune-plugin": "^1.4.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "typescript-require": "^0.2.10",
    "url-slug": "^2.3.0",
    "walk-sync": "^2.0.2",
    "yargs": "^15.1.0"
  }
}
