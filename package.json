{
  "name": "@fiquu/slseed-web-api",
  "title": "Slseed Web API",
  "description": "Serveless Seed Web API project",
  "author": "fiquu <hi@fiquu.dev> (https://github.com/fiquu)",
  "homepage": "https://github.com/fiquu/slseed-web-api",
  "main": "npm start",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/fiquu/slseed-web-api/issues"
  },
  "directories": {
    "test": "test"
  },
  "contributors": [
    "Santiago G. Mar√≠n <santiago.marin@final.cl> (https://github.com/stgogm)"
  ],
  "keywords": [
    "slseed",
    "serverless",
    "sls",
    "api",
    "seed",
    "project"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:fiquu/slseed-web-api.git"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "nyc": {
    "reporter": [
      "html"
    ],
    "include": [
      "service"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "test": "nyc ts-mocha test/**/*.spec.ts",
    "postversion": "git push --tags",
    "domain": "sls create_domain --stage production",
    "clean": "rimraf .build/",
    "prestart": "npm run clean",
    "start": "sls offline start --stage local",
    "setup:stack": "slseed-utils --do setup-stack",
    "setup:env": "slseed-utils --do setup-env",
    "setup": "slseed-utils",
    "predeploy": "npm run clean",
    "deploy": "slseed-utils --do deploy",
    "check": "npm-check --update"
  },
  "dependencies": {
    "@fiquu/database-manager-mongoose": "^1.0.2",
    "@fiquu/is": "^1.2.2",
    "@fiquu/lambda-http-event-handler": "^1.0.1",
    "@fiquu/logger": "^1.0.2",
    "@fiquu/schema-loader-mongoose": "^1.0.2",
    "aws-sdk": "^2.653.0",
    "email-addresses": "^3.1.0",
    "graphql": "^15.0.0",
    "graphql-yoga": "^1.18.3",
    "merge-graphql-schemas": "^1.7.7",
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "@fiquu/eslint-config": "^3.0.2",
    "@fiquu/slseed-web-utils": "^1.0.7",
    "@types/aws-lambda": "^8.10.47",
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.11",
    "@types/mocha": "^7.0.2",
    "@types/ws": "^7.2.3",
    "amazon-cognito-identity-js-node": "0.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "faker": "^4.1.0",
    "glob": "^7.1.6",
    "graphql-tag": "^2.10.3",
    "html-minifier": "^4.0.0",
    "html-to-text": "^5.1.1",
    "inquirer": "^7.1.0",
    "juice": "^6.0.0",
    "mocha": "^7.1.1",
    "mongodb-memory-server-core": "^6.5.2",
    "nyc": "^15.0.1",
    "ora": "^4.0.3",
    "rimraf": "^3.0.2",
    "serverless": "^1.67.0",
    "serverless-domain-manager": "^3.3.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-mocha-plugin": "git+https://github.com/nordcloud/serverless-mocha-plugin.git",
    "serverless-offline": "^6.1.4",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-prune-plugin": "^1.4.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "url-slug": "^2.3.1",
    "walk-sync": "^2.0.2",
    "yargs": "^15.3.1"
  }
}
