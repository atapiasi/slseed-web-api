{
  "name": "@fiquu/slseed-web-api",
  "title": "Slseed Web API",
  "description": "Serveless Seed Web API project",
  "author": "fiquu <hi@fiquu.dev> (https://github.com/fiquu)",
  "homepage": "https://github.com/fiquu/slseed-web-api",
  "main": "npm start",
  "version": "1.1.2",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/fiquu/slseed-web-api/issues"
  },
  "directories": {
    "test": "test"
  },
  "contributors": [
    "Santiago G. Mar√≠n <santiago.marin@final.cl> (https://github.com/stgogm)"
  ],
  "keywords": [
    "slseed",
    "serverless",
    "sls",
    "api",
    "seed",
    "project"
  ],
  "group": {
    "title": "Slseed",
    "name": "slseed"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fiquu/slseed-web-api.git"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "nyc": {
    "reporter": [
      "html"
    ],
    "include": [
      "service"
    ],
    "cache": true,
    "all": true
  },
  "app": {
    "name": "Web API - Fi Slseed",
    "short": "Slseed Web API",
    "description": "Serveless Seed Web API project.",
    "domain": "example.com",
    "subdomain": "www",
    "home": "https://www.example.com",
    "display": "standalone",
    "background": "#f7f6f6",
    "color": "#b5cc18",
    "ogImage": "https://www.example.com/static/images/og-image.jpg"
  },
  "scripts": {
    "test:unit": "npm run test:components && npm run test:schemas && npm run test:functions",
    "test:functions": "sls invoke test --stage local --path test/functions",
    "test:components": "NODE_ENV=local mocha test/components",
    "test:schemas": "NODE_ENV=local mocha test/schemas",
    "postversion": "git push --tags",
    "test": "nyc npm run test:unit",
    "domain": "sls create_domain --stage production",
    "start": "sls offline start --stage local",
    "setup:stack": "node setup/stack",
    "check": "npm-check --update",
    "setup:env": "node setup/env",
    "deploy": "node utils/deploy",
    "setup": "node setup"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "amazon-cognito-identity-js-node": "0.0.3",
    "aws-sdk": "^2.467.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "fi-is": "^1.3.1",
    "inquirer": "^6.3.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ora": "^3.4.0",
    "serverless": "^1.44.1",
    "serverless-domain-manager": "^3.2.2",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-mocha-plugin": "git+https://github.com/nordcloud/serverless-mocha-plugin.git",
    "serverless-offline": "^5.0.1",
    "serverless-prune-plugin": "^1.4.2",
    "slug": "^1.1.0",
    "titleize": "^2.1.0",
    "walk-sync": "^1.1.3",
    "yargs": "^13.2.4"
  }
}
